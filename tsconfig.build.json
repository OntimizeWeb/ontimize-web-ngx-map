{
  "compilerOptions": {
    "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "@angular/*": [
        "node_modules/@angular/*"
      ]
    },
    "outDir": "dist",
    "declaration": true,
    "strict": false,
    "moduleResolution": "node",
    "module": "es2015",
    "target": "es2015",
    "lib": [
      "es2015",
      "dom"
    ],
    "skipLibCheck": true,
    "types": [],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "sourceMap": true,
    "inlineSources": true,
    "allowSyntheticDefaultImports": true
  },
  "files": [
    "src/custom-typings.d.ts",
    "public_api.ts"
  ],
  "angularCompilerOptions": {
    "skipTemplateCodegen": true,
    "annotateForClosureCompiler": true,
    "strictMetadataEmit": true,
    "flatModuleOutFile": "ontimize-web-ngx-map.js",
    "flatModuleId": "ontimize-web-ngx-map"
  }
}