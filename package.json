    "@angular/router": "~8.2.14",
    "@ngx-translate/core": "~11.0.1",
    "@ngx-translate/http-loader": "~4.0.0",
    "hammerjs": "~2.0.8",
    "leaflet": "1.6.0",
    "leaflet-contextmenu": "1.4.0",
    "leaflet-draw": "0.4.14",
    "leaflet-providers": "1.9.1",
    "leaflet.heat": "0.2.0",
